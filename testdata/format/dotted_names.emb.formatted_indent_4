-- '.'-separated names are formatted with no spaces.

import "foo.emb" as f


enum Foo:
    FOUR = 4


struct Bar:
    struct Baz:
        0 [+4]  UInt  field


struct Qux:
    0           [+Foo.FOUR]  Bar.Baz  field
    field.field [+4]         UInt     thing
    if f.Bar.BAZ == f.Bar.BAZ:
        0           [+4]     f.Bar    other
