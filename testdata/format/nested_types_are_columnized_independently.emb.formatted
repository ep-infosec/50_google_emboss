-- Nested types are columnized independently of each other and of the
-- surrounding type.


struct Foo:
  struct Bar:
    0              [+4]  UInt  very_very_long
    very_very_long [+4]  UInt  v

  struct Baz:
    0 [+4]     UInt:32       long
    4 [+long]  UInt:8[long]  data

  0 [+4]  UInt  field
