-- Test struct for 8, 16, 32, and 64 bit Ints.

[$default byte_order: "LittleEndian"]
[(cpp) namespace: "emboss::test"]


struct Sizes:
  0  [+1]  Int  one_byte
  1  [+2]  Int  two_byte
  3  [+3]  Int  three_byte
  6  [+4]  Int  four_byte
  10 [+5]  Int  five_byte
  15 [+6]  Int  six_byte
  21 [+7]  Int  seven_byte
  28 [+8]  Int  eight_byte
